package plausibcheck.adress

import com.sunjinwei.springboot.model.TrafficViolation

rule "rule_01"
    when
        trafficViolation:TrafficViolation(getViolation(actualSpeed >= 100 & actualSpeed <= 200))
        fine : TrafficViolation.Fine();
    then
        trafficViolation.setSuspended("NO");
        fine.setPoints(3);
        fine.setAmount(500);
		System.out.println("规则中打印日志：校验通过!");
end